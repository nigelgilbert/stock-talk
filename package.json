{
  "name": "stock-talk",
  "version": "0.0.3",
  "description": "Rx + twitter and yahoo-finance",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true
    }
  },
  "scripts": {
    "build": "webpack -p --progress",
    "watch": "webpack --watch",
    "start": "nodemon --harmony_destructuring demo.js --ignore client/ --ignore public/",
    "start-dev": "concurrently 'npm run start' 'npm run watch'"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.14",
    "eventemitter3": "^1.1.1",
    "express": "^4.13.4",
    "lokijs": "^1.3.11",
    "memory-cache": "^0.1.4",
    "socket.io": "^1.4.4",
    "twitter": "^1.2.5",
    "utf-8": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "webpack": "^1.12.2",
    "babel-loader": "^6.2.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.3",
    "css-loader": "^0.23.0",
    "concurrently": "^2.0.0",
    "nodemon": "^1.8.1",
    "eslint": "^2.0.0"
  }
}
