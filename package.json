    "rxjs": "^5.0.0-beta.1",
    "socket.io": "^1.4.4",
    "twitter": "^1.2.5",
    "utf-8": "^1.0.0",
    "webcomponents-lite": "^0.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "file-loader": "^0.8.5",
    "html-minifier": "^1.1.1",
    "json-loader": "^0.5.3",
    "nodemon": "^1.8.1",
    "webpack": "^1.12.11"
